<!DOCTYPE html> 
<html> 
  <head> 
    <%= render 'layouts/bootstrap_meta' %> 
    <%= favicon_link_tag %>
    <title>
    	<%= full_title(yield(:base_title), yield(:page_title)) %>
    </title> 
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %> 
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %> 
    <%= csrf_meta_tags %> 
    <%= render 'layouts/shim' %> 
  </head> 
  <body> 
    <%= yield %> 
    <!--<%= debug(params) if Rails.env.development? %> --> 
  </body> 
</html> 
